/views/FileManagerModule/Code/PushAbnormalFile'\\r\\nimport ProduceFileUpload from './views/FileManagerModule/Code/ProduceFileUpload'\\r\\nimport LogisticsUpload from './views/FileManagerModule/Code/LogisticsFileUpload'\\r\\nimport PushOEMFileList from './views/FileManagerModule/Code/PushOEMFileList'\\r\\nimport FileDownloadManager from './views/FileManagerModule/Code/FileDownloadManager.vue'\\r\\nimport InterfaceSystemLog from './views/FileManagerModule/Code/InterfaceSystemLog.vue'\\r\\n\\r\\n\\r\\n\\r\\nVue.use(Router);\\r\\n\\r\\nvar router = new Router({\\r\\n  mode: 'hash',\\r\\n  routes: [\\r\\n    {\\r\\n      path: \\\"/shell-global\\\",\\r\\n      name: \\\"ShellGlobal\\\",\\r\\n      meta: { title: \\\"凭据统计报表\\\" },\\r\\n      component: ShellGlobal\\r\\n    },\\r\\n    {\\r\\n      path: \\\"/file-upload\\\",\\r\\n      name: \\\"FileUpload\\\",\\r\\n      meta: { title: \\\"文件上传管理\\\" },\\r\\n      component: FileUpload\\r\\n    },\\r\\n    {\\r\\n      path: \\\"/push-abnormal\\\",\\r\\n      name: \\\"FileUpload\\\",\\r\\n      meta: { title: \\\"推送异常文件列表\\\" },\\r\\n      component: PushAbnormal\\r\\n    },\\r\\n    {\\r\\n      path: \\\"/produce-upload\\\",\\r\\n      name: \\\"FileUpload\\\",\\r\\n      meta: { title: \\\"生产文件上传文件日志\\\" },\\r\\n      component: ProduceFileUpload\\r\\n    },\\r\\n    {\\r\\n      path: \\\"/logistics-upload\\\",\\r\\n      name: \\\"FileUpload\\\",\\r\\n      meta: { title: \\\"物流文件上传文件日志\\\" },\\r\\n      component: LogisticsUpload\\r\\n    },\\r\\n    {\\r\\n      path: \\\"/push-record\\\",\\r\\n      name: \\\"FileUpload\\\",\\r\\n      meta: { title: \\\"推送文件记录\\\" },\\r\\n      component: PushOEMFileList\\r\\n    },\\r\\n    {\\r\\n      path: \\\"/file-download\\\",\\r\\n      name: \\\"FileDownloadManager\\\",\\r\\n      meta: { title: \\\"文件下载记录\\\" },\\r\\n      component: FileDownloadManager\\r\\n    },\\r\\n    {\\r\\n      path: \\\"/interface-log\\\",\\r\\n      name: \\\"InterfaceSystemLog\\\",\\r\\n 